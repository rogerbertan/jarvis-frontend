    "version": "1.0.0"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{supabase_access_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://your-project.supabase.co",
      "type": "string"
    },
    {
      "key": "supabase_anon_key",
      "value": "your-anon-key-here",
      "type": "string"
    },
    {
      "key": "supabase_access_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "apikey",
                "value": "{{supabase_anon_key}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/v1/token?grant_type=password",
              "host": ["{{base_url}}"],
              "path": ["auth", "v1", "token"],
              "query": [
                {
                  "key": "grant_type",
                  "value": "password"
                }
              ]
            },
            "description": "Authenticate user with email and password"
          },
          "response": []
        },
        {
          "name": "Signup",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "apikey",
                "value": "{{supabase_anon_key}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\",\n  \"data\": {\n    \"name\": \"John Doe\",\n    \"display_name\": \"John Doe\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/v1/signup",
              "host": ["{{base_url}}"],
              "path": ["auth", "v1", "signup"]
            },
            "description": "Register new user and create profile"
          },
          "response": []
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "apikey",
                "value": "{{supabase_anon_key}}"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{supabase_access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/auth/v1/logout",
              "host": ["{{base_url}}"],
              "path": ["auth", "v1", "logout"]
            },
            "description": "Sign out current user"
          },
          "response": []
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "apikey",
                "value": "{{supabase_anon_key}}"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{supabase_access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/auth/v1/user",
              "host": ["{{base_url}}"],
              "path": ["auth", "v1", "user"]
            },
            "description": "Get current authenticated user"
